= form_for @profile do |f|
  - if @profile.errors.any?
    #error_explanation
      %h2= "#{pluralize(@profile.errors.count, "error")} prohibited this profile from being saved:"
      %ul
        - @profile.errors.full_messages.each do |msg|
          %li= msg


  .field
    = f.label :profileImg 
    = f.file_field :profileImg
  
  .field
    = f.label :firstname
    = f.text_field :firstname
  .field
    = f.label :lastname
    = f.text_field :lastname
  .field
    = f.label :nameoffirm
    = f.text_field :nameoffirm
  .field
    = f.label :estyear
    = f.text_field :estyear
  .field
    = f.label :website
    = f.text_field :website
  .field
    = f.label :pan
    = f.text_field :pan
  .field
    = f.label :tanvat
    = f.text_field :tanvat
  .field
    = f.label :bankacc
    = f.text_field :bankacc
  .field
    = f.label :billingaddress
    = f.text_field :billingaddress
  .field
    = f.label  :deliveryaddress
    = f.text_field  :deliveryaddress
  .actions
    = f.submit 'Save'
